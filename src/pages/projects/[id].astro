---
import { SectionPageComponent } from '../../components/SectionPageComponent';
import { projectsData } from '../../data/Scholarship'
import Layout from '../../layouts/Layout.astro';

export async function getStaticPaths() {
  return [
    { params: { id: 'danone' } },
  ];
}

const { id } = Astro.params;
const projects = projectsData.find((project) => project.id === id);
---
<Layout title="My portfolio.">
  <SectionPageComponent info={ projects } />
</Layout>